<p id="notice"><%= notice %></p>

<h1>Listing Workflow Masters</h1>

<table>
  <thead>
    <tr>
      <th>Company</th>
      <th>Name</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @workflow_masters.each do |workflow_master| %>
      <tr>
        <td><%= workflow_master.company_id %></td>
        <td><%= workflow_master.name %></td>
        <td><%= workflow_master.status %></td>
        <td><%= link_to 'Show', workflow_master %></td>
        <td><%= link_to 'Edit', edit_workflow_master_path(workflow_master) %></td>
        <td><%= link_to 'Destroy', workflow_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Workflow master', new_workflow_master_path %>
