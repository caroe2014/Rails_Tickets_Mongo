<!DOCTYPE html>
<html>
<head>
  <title>Ticketmongo</title>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>


</head>
<body ng-app="TicketModule">
<%   if user_signed_in? %>
<%=     render 'layouts/topmenusignin'  %>
<%   else %>
<%=     render 'layouts/topmenunosign'  %>
<%   end %>

<%= yield %>

</body>
</html>
