<%= simple_form_for(@workflow_detail) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :company_id %>
    <%= f.input :workflow_master_id %>
    <%= f.input :next %>
    <%= f.input :previous %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
